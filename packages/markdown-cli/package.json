{
  "name": "@accordproject/markdown-cli",
  "version": "0.11.3",
  "description": "A framework for transforming markdown",
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "markus": "./index.js"
  },
  "files": [
    "bin",
    "lib",
    "umd"
  ],
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint",
    "lint": "eslint .",
    "postlint": "npm run licchk",
    "licchk": "license-check",
    "test": "mocha",
    "test:cov": "nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/accordproject/markdown-transform.git"
  },
  "keywords": [
    "accord",
    "markdown",
    "commonmark"
  ],
  "author": "accordproject.org",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/accordproject/markdown-transform/issues"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-things": "0.2.0",
    "eslint": "6.0.1",
    "jsdoc": "3.6.3",
    "license-check": "1.1.5",
    "mocha": "6.1.4",
    "nyc": "14.1.1",
    "tsd-jsdoc": "^2.3.0"
  },
  "dependencies": {
    "@accordproject/concerto-core": "^0.82.7",
    "@accordproject/markdown-transform": "0.11.3",
    "@accordproject/markdown-template": "0.11.2",
    "jsome": "2.5.0",
    "winston": "3.2.1",
    "yargs": "13.2.4"
  },
  "license-check-config": {
    "src": [
      "**/*.js",
      "!./scripts/**/*",
      "!./.travis/**/*",
      "!./test/**/*",
      "!./coverage/**/*",
      "!./node_modules/**/*",
      "!./out/**/*",
      "!./lib/**/*",
      "!./umd/**/*",
      "!./index.js"
    ],
    "path": "header.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  },
  "nyc": {
    "produce-source-map": "true",
    "sourceMap": "inline",
    "reporter": [
      "lcov",
      "text-summary",
      "html",
      "json"
    ],
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "scripts/**/*.js"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 55,
    "branches": 40,
    "functions": 30,
    "lines": 55
  }
}