            {
              "$class": "org.accordproject.commonmark@0.5.0.Text",
              "text": "\"Party A\" that it is either accepting or rejecting the"
            },
            {
              "$class": "org.accordproject.commonmark@0.5.0.Softbreak"
            },
            {
              "$class": "org.accordproject.commonmark@0.5.0.Text",
              "text": "\"Widgets\"."
            }
