namespace io.clause.signature.namedate

import org.accordproject.contract.* from https://models.accordproject.org/accordproject/contract.cto
import org.accordproject.runtime.* from https://models.accordproject.org/accordproject/runtime.cto

@TestAnnot("foo",3,true,false,bar)
concept Test {
	@BoolAnnot(true)
	@StringAnnot("foo")
	@NumberAnnot(3.14)
	@IdentAnnot(identifier)
	o Boolean bool

	@ArrayAnnot(3)
	o Double[] doubles
}

/**
 * A Signature block
 */
@ClauseAnnot("bar",-3,false,true,foo)
asset SignatureClause extends Clause {

  @SignatureVariable()
  --> Party party

  @DocuSignTab("Text")
  @SignatureVariable()
  o String favoriteColor

  @DocuSignTab("Title", "optional")
  @SignatureVariable()
  o String title

  @DocuSignTab("SignHere")
  @SignatureVariable()
  o String signature

  @DocuSignTab("FullName")
  @SignatureVariable()
  o String name

  @DocuSignTab("DateSigned")
  @SignatureVariable()
  o String date

	o Test test
}