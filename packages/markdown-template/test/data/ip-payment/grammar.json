{
  "$class": "org.accordproject.commonmark.Document",
  "xmlns": "http://commonmark.org/xml/1.0",
  "nodes": [
    {
      "$class": "org.accordproject.templatemark.ClauseDefinition",
      "name": "top",
      "elementType": "org.accordproject.ippayment.IntellectualPropertyPaymentClause",
      "nodes": [
        {
          "$class": "org.accordproject.commonmark.Paragraph",
          "nodes": [
            {
              "$class": "org.accordproject.commonmark.Text",
              "text": "Payments."
            }
          ]
        },
        {
          "$class": "org.accordproject.commonmark.List",
          "type": "ordered",
          "start": "1",
          "tight": "false",
          "delimiter": "period",
          "nodes": [
            {
              "$class": "org.accordproject.commonmark.Item",
              "nodes": [
                {
                  "$class": "org.accordproject.commonmark.Paragraph",
                  "nodes": [
                    {
                      "$class": "org.accordproject.commonmark.Text",
                      "text": "Royalty. On or before the last business day of each Quarterly Period during the Term and any Sell-off Period, Licensee shall pay to Licensor a royalty of "
                    },
                    {
                      "$class": "org.accordproject.templatemark.VariableDefinition",
                      "name": "royaltyText",
                      "elementType": "String"
                    },
                    {
                      "$class": "org.accordproject.commonmark.Text",
                      "text": " percent ("
                    },
                    {
                      "$class": "org.accordproject.templatemark.VariableDefinition",
                      "name": "royaltyRate",
                      "elementType": "Double"
                    },
                    {
                      "$class": "org.accordproject.commonmark.Text",
                      "text": "%) of the Net Sales Price for the respective preceding Quarterly Period."
                    }
                  ]
                }
              ]
            },
            {
              "$class": "org.accordproject.commonmark.Item",
              "nodes": [
                {
                  "$class": "org.accordproject.commonmark.Paragraph",
                  "nodes": [
                    {
                      "$class": "org.accordproject.commonmark.Text",
                      "text": "Sublicensing Revenue. On or before the last business day of each Quarterly Period during the Term and any Sell-off Period, Licensee shall pay to Licensor "
                    },
                    {
                      "$class": "org.accordproject.templatemark.VariableDefinition",
                      "name": "sublicensingRoyaltyText",
                      "elementType": "String"
                    },
                    {
                      "$class": "org.accordproject.commonmark.Text",
                      "text": " percent ("
                    },
                    {
                      "$class": "org.accordproject.templatemark.VariableDefinition",
                      "name": "sublicensingRoyaltyRate",
                      "elementType": "Double"
                    },
                    {
                      "$class": "org.accordproject.commonmark.Text",
                      "text": "%) of any royalties and fees received by Licensee from sublicensing any rights granted to it under this Agreement during the respective preceding Quarterly Period."
                    }
                  ]
                }
              ]
            },
            {
              "$class": "org.accordproject.commonmark.Item",
              "nodes": [
                {
                  "$class": "org.accordproject.commonmark.Paragraph",
                  "nodes": [
                    {
                      "$class": "org.accordproject.commonmark.Text",
                      "text": "Taxes. If Licensee is required by applicable Law to withhold taxes in connection with any sums payable to Licensor under this Agreement, Licensee may deduct the amount of the withholding from the payment it otherwise would have made to Licensor under this Agreement."
                    }
                  ]
                }
              ]
            },
            {
              "$class": "org.accordproject.commonmark.Item",
              "nodes": [
                {
                  "$class": "org.accordproject.commonmark.Paragraph",
                  "nodes": [
                    {
                      "$class": "org.accordproject.commonmark.Text",
                      "text": "Manner of Payment."
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "$class": "org.accordproject.commonmark.Paragraph",
          "nodes": [
            {
              "$class": "org.accordproject.commonmark.Text",
              "text": "(a) Royalties and any other sums payable under this Section shall be paid within "
            },
            {
              "$class": "org.accordproject.templatemark.WithDefinition",
              "name": "paymentPeriod",
              "elementType": "org.accordproject.time.Duration",
              "nodes": [
                {
                  "$class": "org.accordproject.templatemark.VariableDefinition",
                  "name": "amount",
                  "elementType": "Long"
                },
                {
                  "$class": "org.accordproject.commonmark.Text",
                  "text": " "
                },
                {
                  "$class": "org.accordproject.templatemark.EnumVariableDefinition",
                  "enumValues": [
                    "seconds",
                    "minutes",
                    "hours",
                    "days",
                    "weeks"
                  ],
                  "name": "unit"
                }
              ]
            },
            {
              "$class": "org.accordproject.commonmark.Text",
              "text": " following the end of each Quarterly Period in US dollars."
            }
          ]
        },
        {
          "$class": "org.accordproject.commonmark.Paragraph",
          "nodes": [
            {
              "$class": "org.accordproject.commonmark.Text",
              "text": "(b) If Licensee is prohibited by a governmental authority in any country from making any payment due under this Section, then within the prescribed period for making the payment Licensee shall request permission from the governmental authority to make the payment and shall make the payment within "
            },
            {
              "$class": "org.accordproject.templatemark.WithDefinition",
              "name": "paymentPeriodWithPermission",
              "elementType": "org.accordproject.time.Duration",
              "nodes": [
                {
                  "$class": "org.accordproject.templatemark.VariableDefinition",
                  "name": "amount",
                  "elementType": "Long"
                },
                {
                  "$class": "org.accordproject.commonmark.Text",
                  "text": " "
                },
                {
                  "$class": "org.accordproject.templatemark.EnumVariableDefinition",
                  "enumValues": [
                    "seconds",
                    "minutes",
                    "hours",
                    "days",
                    "weeks"
                  ],
                  "name": "unit"
                }
              ]
            },
            {
              "$class": "org.accordproject.commonmark.Text",
              "text": " business days after receiving permission. If such permission is not received within "
            },
            {
              "$class": "org.accordproject.templatemark.WithDefinition",
              "name": "paymentPeriodWithPermission",
              "elementType": "org.accordproject.time.Duration",
              "nodes": [
                {
                  "$class": "org.accordproject.templatemark.VariableDefinition",
                  "name": "amount",
                  "elementType": "Long"
                },
                {
                  "$class": "org.accordproject.commonmark.Text",
                  "text": " "
                },
                {
                  "$class": "org.accordproject.templatemark.EnumVariableDefinition",
                  "enumValues": [
                    "seconds",
                    "minutes",
                    "hours",
                    "days",
                    "weeks"
                  ],
                  "name": "unit"
                }
              ]
            },
            {
              "$class": "org.accordproject.commonmark.Text",
              "text": " business days after Licensee's request, then Licensee, at Licensor's option, shall either deposit the payment in the currency of the relevant country in a bank account within that country designated by Licensor or make the payment to an associated company of Licensor designated by Licensor and having an office in the relevant country or in another country designated by Licensor."
            }
          ]
        },
        {
          "$class": "org.accordproject.commonmark.List",
          "type": "ordered",
          "start": "5",
          "tight": "true",
          "delimiter": "period",
          "nodes": [
            {
              "$class": "org.accordproject.commonmark.Item",
              "nodes": [
                {
                  "$class": "org.accordproject.commonmark.Paragraph",
                  "nodes": [
                    {
                      "$class": "org.accordproject.commonmark.Text",
                      "text": "Royalty Statements. At the same time as payments are made under Section 4., Licensee shall submit a statement showing the total Net Sales Price of Licensed Product[s] received by Licensee and its Affiliates and sublicensees and the total amount of any sublicense royalties and other fees received by Licensee in the relevant Quarterly Period."
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}