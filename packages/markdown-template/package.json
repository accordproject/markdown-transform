{
  "name": "@accordproject/markdown-template",
  "version": "0.13.0-alpha.1",
  "description": "A framework for transforming markdown",
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin",
    "lib",
    "umd"
  ],
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint",
    "lint": "eslint .",
    "postlint": "npm run licchk",
    "licchk": "license-check",
    "test": "mocha",
    "test:cov": "npm run lint && nyc mocha",
    "jsdoc": "jsdoc -c jsdoc.json package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/accordproject/markdown-transform.git",
    "directory": "packages/markdown-template"
  },
  "keywords": [
    "accord-project",
    "markdown",
    "markdown-it",
    "commonmark"
  ],
  "author": "accordproject.org",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/accordproject/markdown-transform/issues"
  },
  "homepage": "https://github.com/accordproject/markdown-transform",
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-string": "^1.5.0",
    "chai-things": "0.2.0",
    "eslint": "6.0.1",
    "handlebars": "^4.4.5",
    "jest": "^24.8.0",
    "jest-diff": "^24.8.0",
    "jsdoc": "3.6.3",
    "license-check": "1.1.5",
    "mocha": "6.1.4",
    "nyc": "14.1.1"
  },
  "dependencies": {
    "@accordproject/concerto-core": "1.0.0-alpha.5",
    "@accordproject/markdown-cicero": "0.13.0-alpha.1",
    "@accordproject/markdown-common": "0.13.0-alpha.1",
    "@accordproject/markdown-it-template": "0.13.0-alpha.1",
    "dayjs": "1.10.2",
    "markdown-it": "^11.0.0",
    "parsimmon": "^1.13.0",
    "uuid": "3.3.2"
  },
  "license-check-config": {
    "src": [
      "**/*.js",
      "!./scripts/**/*",
      "!./test/**/*",
      "!./coverage/**/*",
      "!./node_modules/**/*",
      "!./out/**/*",
      "!./umd/**/*",
      "!./bin/index.js"
    ],
    "path": "header.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  },
  "nyc": {
    "produce-source-map": "true",
    "sourceMap": "inline",
    "reporter": [
      "lcov",
      "text",
      "text-summary",
      "html",
      "json"
    ],
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "scripts/**/*.js"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 87,
    "branches": 76,
    "functions": 84,
    "lines": 87
  }
}
