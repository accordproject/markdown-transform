{
  "name": "@accordproject/markdown-template",
  "version": "0.14.0",
  "description": "A framework for transforming markdown",
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin",
    "lib",
    "umd"
  ],
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint",
    "lint": "eslint .",
    "postlint": "npm run licchk",
    "licchk": "license-check-and-add",
    "test": "mocha",
    "test:cov": "npm run lint && nyc mocha",
    "jsdoc": "jsdoc -c jsdoc.json package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/accordproject/markdown-transform.git",
    "directory": "packages/markdown-template"
  },
  "keywords": [
    "accord-project",
    "markdown",
    "markdown-it",
    "commonmark"
  ],
  "author": "accordproject.org",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/accordproject/markdown-transform/issues"
  },
  "homepage": "https://github.com/accordproject/markdown-transform",
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-string": "^1.5.0",
    "chai-things": "0.2.0",
    "eslint": "^7.32.0",
    "handlebars": "^4.4.5",
    "jest": "^27.0.6",
    "jest-diff": "^27.0.6",
    "jsdoc": "^3.6.7",
    "license-check-and-add": "2.3.6",
    "mocha": "6.1.4",
    "nyc": "14.1.1"
  },
  "dependencies": {
    "@accordproject/concerto-core": "1.1.0",
    "@accordproject/markdown-cicero": "0.14.0",
    "@accordproject/markdown-common": "0.14.0",
    "@accordproject/markdown-it-template": "0.14.0",
    "dayjs": "1.10.2",
    "markdown-it": "^12.2.0",
    "parsimmon": "1.16.0",
    "uuid": "3.3.2"
  },
  "license-check-and-add-config": {
    "folder": "./lib",
    "license": "header.txt",
    "exact_paths_method": "EXCLUDE",
    "exact_paths": [
      "externalModels/.npmignore",
      "externalModels/.gitignore",
      "coverage",
      "index.d.ts",
      "./system",
      "LICENSE",
      "node_modules",
      ".nyc-output",
      "out",
      ".tern-project"
    ],
    "file_type_method": "EXCLUDE",
    "file_types": [
      ".yml",
      ".yaml",
      ".zip",
      ".tgz",
      ".snap"
    ],
    "insert_license": false,
    "license_formats": {
      "js|njk|pegjs|cto|acl|qry": {
        "prepend": "/*",
        "append": " */",
        "eachLine": {
          "prepend": " * "
        }
      },
      "npmrc|editorconfig|txt": {
        "eachLine": {
          "prepend": "# "
        }
      },
      "md": {
        "file": "header.md"
      }
    }
  },
  "nyc": {
    "produce-source-map": "true",
    "sourceMap": "inline",
    "reporter": [
      "lcov",
      "text",
      "text-summary",
      "html",
      "json"
    ],
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "scripts/**/*.js"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 87,
    "branches": 76,
    "functions": 84,
    "lines": 87
  }
}
