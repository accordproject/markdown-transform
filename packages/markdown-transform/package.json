{
  "name": "@accordproject/markdown-transform",
  "version": "0.11.3",
  "description": "API for transforming markdown data",
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin",
    "lib",
    "umd"
  ],
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint && npm run generateDiagram",
    "lint": "eslint .",
    "generateDiagram": "node ./scripts/generateDiagram.js",
    "postlint": "npm run licchk",
    "licchk": "license-check",
    "test": "mocha",
    "test:cov": "nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/accordproject/markdown-transform.git"
  },
  "keywords": [
    "accord-project",
    "markdown",
    "markdown-it",
    "commonmark"
  ],
  "author": "accordproject.org",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/accordproject/markdown-transform/issues"
  },
  "devDependencies": {
    "axios": "^0.19.2",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-string": "^1.5.0",
    "chai-things": "0.2.0",
    "eslint": "6.0.1",
    "jsdoc": "3.6.3",
    "license-check": "1.1.5",
    "mocha": "6.1.4",
    "nyc": "14.1.1",
    "plantuml-encoder": "^1.4.0",
    "tsd-jsdoc": "^2.3.0"
  },
  "dependencies": {
    "@accordproject/concerto-core": "^0.82.7",
    "@accordproject/markdown-cicero": "0.11.3",
    "@accordproject/markdown-common": "0.11.3",
    "@accordproject/markdown-docx": "0.11.3",
    "@accordproject/markdown-html": "0.11.3",
    "@accordproject/markdown-pdf": "0.11.3",
    "@accordproject/markdown-slate": "0.11.3",
    "@accordproject/markdown-template": "0.11.3",
    "moment-mini": "2.22.1",
    "dijkstrajs": "^1.0.1"
  },
  "license-check-config": {
    "src": [
      "**/*.js",
      "!./scripts/**/*",
      "!./.travis/**/*",
      "!./test/**/*",
      "!./coverage/**/*",
      "!./node_modules/**/*",
      "!./out/**/*",
      "!./umd/**/*",
      "!./index.js"
    ],
    "path": "header.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  },
  "nyc": {
    "produce-source-map": "true",
    "sourceMap": "inline",
    "reporter": [
      "lcov",
      "text-summary",
      "html",
      "json"
    ],
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "scripts/**/*.js"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 90,
    "branches": 88,
    "functions": 88,
    "lines": 90
  }
}